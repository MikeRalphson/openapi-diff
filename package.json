{
  "name": "openapi-diff",
  "version": "0.4.0",
  "description": "A CLI tool to identify differences between Swagger/OpenAPI specs.",
  "bin": {
    "openapi-diff": "./bin/openapi-diff"
  },
  "scripts": {
    "release-major": "./node_modules/.bin/gulp release --type major",
    "release-minor": "./node_modules/.bin/gulp release --type minor",
    "release-patch": "./node_modules/.bin/gulp release --type patch",
    "test": "./node_modules/.bin/gulp",
    "watch": "./node_modules/.bin/gulp watch",
    "watch-e2e": "./node_modules/.bin/gulp watch-e2e"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org/atlassian/openapi-diff.git"
  },
  "keywords": [
    "diff",
    "difference",
    "OpenAPI",
    "Swagger"
  ],
  "author": {
    "name": "Mauri Edo (Atlassian)",
    "email": "medo@atlassian.com"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.8.4"
  },
  "homepage": "https://bitbucket.org/atlassian/openapi-diff",
  "devDependencies": {
    "@types/commander": "^2.9.1",
    "@types/deep-diff": "0.0.30",
    "@types/express": "^4.0.36",
    "@types/jasmine": "^2.5.52",
    "@types/lodash": "^4.14.66",
    "@types/proxyquire": "^1.3.27",
    "@types/q": "^1.0.2",
    "@types/request": "0.0.45",
    "@types/swagger-schema-official": "^2.0.5",
    "@types/verror": "^1.10.0",
    "conventional-changelog-lint": "^1.1.9",
    "del": "^3.0.0",
    "express": "^4.15.3",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-conventional-changelog": "^1.1.3",
    "gulp-git": "^2.4.1",
    "gulp-jasmine": "^2.4.2",
    "gulp-tslint": "^8.1.1",
    "gulp-typescript": "^3.1.7",
    "jasmine": "^2.7.0",
    "minimist": "^1.2.0",
    "run-sequence": "^1.2.2",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  },
  "dependencies": {
    "commander": "^2.10.0",
    "deep-diff": "0.3.4",
    "lodash": "^4.17.4",
    "openapi3-ts": "^0.2.1",
    "q": "^1.5.0",
    "request": "^2.81.0",
    "verror": "^1.10.0"
  }
}
