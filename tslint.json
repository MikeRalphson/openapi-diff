{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest"
    ],
    "jsRules": {},
    "rules": {
//      "cyclomatic-complexity": [true, 4],
      "interface-name": [true, "never-prefix"],
      "no-console": false,
      "object-literal-key-quotes": [true, "as-needed"],
      "quotemark": [true, "single"],
      "trailing-comma": [true, {"multiline": "never", "singleline": "never"}]
    },
    "rulesDirectory": []
}